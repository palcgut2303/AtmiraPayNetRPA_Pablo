<Window x:Class="AtmiraPayNetRPA.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AtmiraPayNetRPA"
        mc:Ignorable="d"
        Title="MainWindow" Height="733" Width="1098">
    <Grid Margin="-394,10,10,-10">


        <Label Content="Inicio de Sesiòn" Margin="442,0,777,0" VerticalAlignment="Top" Height="49" FontSize="30" FontWeight="Bold"/>
        <Label Content="Email" HorizontalAlignment="Left" Margin="444,93,0,0" VerticalAlignment="Top" Height="49" Width="115" FontSize="24"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="442,192,0,0" VerticalAlignment="Top" Height="49" Width="115" FontSize="24"/>
        <TextBox x:Name="txtEmailLogin" HorizontalAlignment="Left" Margin="444,147,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183" Height="40"/>
        <TextBox x:Name="txtPasswordLogin" HorizontalAlignment="Left" Margin="444,242,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183" Height="40" />

        <Label Content="Creacion de PAGO" HorizontalAlignment="Left" Margin="922,0,0,0" VerticalAlignment="Top" Height="49" Width="268" FontSize="30" FontWeight="Bold" RenderTransformOrigin="0.5,0.5" />
        <Label Content="IBAN ORIGEN" HorizontalAlignment="Left" Margin="792,107,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" >

        </Label>
        <Label Content="NOMBRE DEL BANCO ORIGEN" HorizontalAlignment="Left" Margin="747,188,0,0" VerticalAlignment="Top"/>
        <Label Content="PAIS ORIGEN" HorizontalAlignment="Left" Margin="793,299,0,0" VerticalAlignment="Top"/>

        <Label Content="IBAN DESTINO" HorizontalAlignment="Left" Margin="1040,110,0,0" VerticalAlignment="Top"/>
        <Label Content="NOMBRE DEL BANCO DESTINO" HorizontalAlignment="Left" Margin="996,188,0,0" VerticalAlignment="Top"/>
        <Label Content="PAIS DESTINO" HorizontalAlignment="Left" Margin="1039,297,0,0" VerticalAlignment="Top"/>

        <Label Content="IBAN INTERMEDIARIO" HorizontalAlignment="Left" Margin="1261,105,0,0" VerticalAlignment="Top"/>
