        <Label Content="BANCO INTERM." HorizontalAlignment="Left" Margin="851,66,0,0" VerticalAlignment="Top" Height="40" Width="173" FontSize="20" FontWeight="Bold"/>
        <Rectangle HorizontalAlignment="Left" Height="374" Margin="575,67,0,0" Stroke="Black" VerticalAlignment="Top" Width="206"/>
        <Rectangle HorizontalAlignment="Left" Height="375" Margin="318,66,0,0" Stroke="Black" VerticalAlignment="Top" Width="207"/>
        <Rectangle HorizontalAlignment="Left" Height="295" Margin="830,67,0,0" Stroke="Black" VerticalAlignment="Top" Width="216" RenderTransformOrigin="0.478,0.524"/>
        <Label Content="Inicio de Sesión" HorizontalAlignment="Left" Margin="114,67,0,0" VerticalAlignment="Top" Width="172" FontSize="22" FontWeight="Bold"/>
        <Label Name="ErrorLabel" Content="An error occurred!" Foreground="Red" Visibility="Collapsed"   HorizontalAlignment="Left" VerticalAlignment="Top" Margin="147,359,0,0"/>
        <Label Name="SuccesLabel" Content="Completed Test!" Foreground="Green" Visibility="Collapsed"   HorizontalAlignment="Left" VerticalAlignment="Top" Margin="147,359,0,0"/>
        <ScrollViewer   HorizontalScrollBarVisibility="Auto" Margin="114,402,824,192" Width="183" Height="123">
            <TextBlock Name="ErrorTextBlock" Foreground="Red" Visibility="Collapsed"  TextWrapping="Wrap" Text="TextBlock" Height="269" Width="173"/>
        </ScrollViewer>
        <ScrollViewer HorizontalScrollBarVisibility="Auto" Margin="114,402,824,192" Width="183" Height="123">
            <TextBlock Name="SuccessTextBlock" Foreground="Green" Visibility="Collapsed"  TextWrapping="Wrap" Text="TextBlock" Height="269" Width="173"/>
        </ScrollViewer>
        <Button Content="Ver Lista de Pagos" HorizontalAlignment="Left" Margin="114,551,0,0" VerticalAlignment="Top" Height="56" Width="171" FontSize="20" Click="Button_Click_1"/>
        <Label Content="Para Ver Lista de Pagos, solo rellene los campos de Inicio de Sesion." FontSize="15" HorizontalAlignment="Left" Margin="114,612,0,0" VerticalAlignment="Top" Height="45" Width="456"/>
        <Label Content="Para Crear Pago, rellena tanto los datos de inicio de sesion como los del formulario" FontSize="15" HorizontalAlignment="Left" Margin="115,652,0,0" VerticalAlignment="Top" Height="45" Width="573"/>


    </Grid>
</Window>
